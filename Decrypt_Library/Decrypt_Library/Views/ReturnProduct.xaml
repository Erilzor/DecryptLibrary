<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Decrypt_Library.Views.ReturnProduct">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="Product" Placeholder="Streckod på produkt" IsVisible="true" />
            <Button x:Name="ReturnProductId" Text="Lämna tillbaka" IsVisible="true" Clicked="ReturnProduct_Clicked"  BackgroundColor="Black" TextColor="White" />
            <Button x:Name="ReturnNewProduct" Text="Återlämna en till produkt" IsVisible="false" Clicked="ReturnNewProduct_Clicked"  BackgroundColor="Black" TextColor="White" />
            <Button x:Name="CheckOut" Text="Klar" IsVisible="false" Clicked="CheckOut_Clicked"  BackgroundColor="Black" TextColor="White" />
            <Label x:Name="Headline" Text="ÅTERLÄMNINGSKVITTO" FontSize="Large" IsVisible="false"/>
            <Label x:Name="Receipt" Text="" IsVisible="false"/>
            <Button x:Name="StartAgain" Text="Starta om" Clicked="StartAgain_Clicked" IsVisible="false" BackgroundColor="Black" TextColor="White"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>